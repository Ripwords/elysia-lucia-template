{
  "name": "backend",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --target=bun --minify --splitting --outdir=dist",
    "start": "bun run build && NODE_ENV=production bun dist/index.js",
    "test": "bun lint && bun test --env-file=.env.test",
    "lint": "eslint . --fix",
    "db": "bun x prisma db"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.1.0",
    "@elysiajs/cron": "^1.1.0",
    "@elysiajs/swagger": "^1.1.0",
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@prisma/client": "^5.17.0",
    "adm-zip": "^0.5.14",
    "bson": "^6.8.0",
    "elysia-rate-limit": "^4.1.0",
    "logestic": "^1.2.2",
    "lucia": "^3.2.0",
    "nodemailer": "^6.9.14",
    "oslo": "^1.2.1",
    "prisma": "^5.17.0",
    "unify-errors": "^1.3.37",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/adm-zip": "^0.5.5",
    "@types/nodemailer": "^6.4.15",
    "bun-types": "1.1.20",
    "eslint": "9.7.0",
    "globals": "^15.8.0",
    "typescript-eslint": "^7.16.1"
  },
  "module": "src/index.js",
  "type": "module",
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}